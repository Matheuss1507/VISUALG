Algoritmo "semnome"


Var

 contador,limite,resultado,tabuada  : inteiro

Inicio

 contador:=0
 limite:=10
 
 escreval("Qual tabuada voce quer? ")
 leia(tabuada)
		
 enquanto (contador<=limite) faca
		
 resultado:=tabuada * contador
 escreval(tabuada , " X " , contador , " = " , resultado )
 contador := contador+1
 
 fimenquanto



Fimalgoritmo